{
    "name": "clockbot",
    "version": "1.0.13",
    "type": "module",
    "description": "a Discord.js template supporting locales with dynamic events and /commands",
    "main": "clockbot.js",
    "scripts": {
        "start": "node clockbot.js",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "keywords": [
        "discord",
        "discord.js",
        "bot",
        "template",
        "clockbot",
        "locales",
        "events",
        "commands"
    ],
    "author": "Russell Purinton <russell.purinton@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/rpurinton/clockbot.git"
    },
    "bugs": {
        "url": "https://github.com/rpurinton/clockbot/issues"
    },
    "license": "MIT",
    "dependencies": {
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
        "moment-timezone": "^0.6.0",
        "mysql2": "^3.14.1",
        "node-fetch": "^3.3.2",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "jest": "^29.7.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs"
        ]
    }
}